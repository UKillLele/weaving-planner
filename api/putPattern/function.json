{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "patternDataIn",
      "databaseName": "weaving-planner-database",
      "collectionName": "weaving-planner-container",
      "connectionStringSetting": "CosmosConnectionString",
      "createIfNotExists": false,
      "sqlQuery": "SELECT TOP 1 * FROM c WHERE c.id = {id}",
      "partitionKey": "/id"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "patternData",
      "databaseName": "weaving-planner-database",
      "collectionName": "weaving-planner-container",
      "connectionStringSetting": "CosmosConnectionString",
      "createIfNotExists": false,
      "partitionKey": "/id"
    }
  ],
  "scriptFile": "../dist/postPattern/index.js"
}
