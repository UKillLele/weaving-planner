{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "cosmosDB",
      "name": "patternData",
      "databaseName": "weaving-planner-database",
      "collectionName": "weaving-planner-container",
      "connectionStringSetting": "CosmosConnectionString",
      "sqlQuery": "SELECT * FROM c WHERE c.userId = {for} AND c.id = {id}",
      "direction": "in"
    }
  ],
  "scriptFile": "../dist/getPatterns/index.js"
}
